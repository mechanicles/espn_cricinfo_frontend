<h1>Matches</h1>
<table>
  <tbody>
    <% @matches.each do |match| %>
      <tr>
        <td><%= match["team1"] %> vs <%= match["team2"] %></td>
        <td>Total Run for <%= match["team1"] %> : <%= match["total_run_for_team1"] %></td>
        <td>Total Run for <%=  match["team2"] %> : <%= match["total_run_for_team2"] %></td>
        <td><%= link_to "Show", "#" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

